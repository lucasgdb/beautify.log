{
	"name": "beautify.log",
	"description": "A Node.js lib to beautify the console.log from JavaScript with colors, making it easy and useful.",
	"version": "2.1.2",
	"main": "dist/index.js",
	"types": "index.d.ts",
	"repository": "git@github.com:lucasgdb/beautify.log.git",
	"author": "Lucas Bittencourt <lucasgdbittencourt@gmail.com>",
	"license": "MIT",
	"scripts": {
		"dev": "sucrase-node src/index.ts",
		"build": "babel src --out-dir dist --copy-files --extensions '.ts,.js'",
		"start": "node ."
	},
	"keywords": [
		"color",
		"terminal",
		"escape",
		"node",
		"console-log",
		"typescript"
	],
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/preset-env": "^7.9.5",
		"@babel/preset-typescript": "^7.9.0",
		"@types/node": "^13.11.1",
		"babel-preset-minify": "^0.5.1",
		"sucrase": "^3.13.0",
		"tslint": "^6.1.1",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.8.3"
	}
}
